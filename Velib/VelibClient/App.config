<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="VelibService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8733/Velibo" binding="basicHttpBinding"
                bindingConfiguration="VelibService" contract="ServiceVelib.IVelibIntermediary"
                name="VelibService" />
        </client>
    </system.serviceModel>
</configuration>