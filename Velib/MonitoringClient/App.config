<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7"/>
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="Monitoring" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8733/Monitoring" binding="basicHttpBinding"
                bindingConfiguration="Monitoring" contract="VelibMonitoring.IMonitoring"
                name="Monitoring" />
        </client>
    </system.serviceModel>
</configuration>
